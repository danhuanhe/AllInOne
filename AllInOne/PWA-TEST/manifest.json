{
  "scope": "/",//用来设置manifest对于网站的作用范围。
  "name": "pwa-test",//用来描述应用的名称，会显示在各类提示的标题位置和启动画面中
  "short_name": "pwatest",//用来描述应用的短名字。当应用的名字过长，在桌面图标上无法全部显示时，会用short_name中定义的来显示。
  "start_url": "/?from=homescreen",//用来描述当用户从设备的主屏幕点击图标进入时，出现的第一个画面
  "display": "standalone",//用来指定 Web App 从主屏幕点击启动后的显示类型
  "description": "pwa-test pwa-test.",
  "dir": "rtl",//设置文字的显示方向
  "lang": "cn",
  "orientation": "portrait",//Web App的在屏幕上的显示方向。
  "theme_color": "#3f51b5",//定义和background_color一样的CSS颜色值，用于显示Web App的主题色，显示在banner位置。
  "background_color": "#fff",//值为CSS的颜色值，用来设置Web App启动画面的背景颜色。
  //用来设置Web App的图标集合
  "icons": [{
    "src": "res/img/icon-36x36.png",
    "sizes": "36x36",
    "type": "image/png"
  }, {
    "src": "res/img/icon-48x48.png",
    "sizes": "48x48",
    "type": "image/png"
  }, {
    "src": "res/img/icon-72x72.png",
    "sizes": "72x72",
    "type": "image/png"
  }, {
    "src": "res/img/icon-96x96.png",
    "sizes": "96x96",
    "type": "image/png"
  }, {
    "src": "res/img/icon-144x144.png",
    "sizes": "144x144",
    "type": "image/png"
  }, {
    "src": "res/img/icon-192x192.png",
    "sizes": "192x192",
    "type": "image/png"
  }, {
    "src": "res/img/icon-256x256.png",
    "sizes": "256x256",
    "type": "image/png"
  }]
}