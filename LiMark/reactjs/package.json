{
  "name": "callcenter",
  "version": "1.0.0",
  "description": "callcenter base react&ppfish",
  "main": "index.js",
  "scripts": {
    "imagemin": "babel-node tools/imagemin.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "open:src": "babel-node --max_old_space_size=2048 tools/srcServer.js",
    "start:debug": "babel-node $NODE_DEBUG_OPTION tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "stylelint": "stylelint \"source/**/*.less\"",
    "lint": "esw webpack.config.* source tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "build:dll": "babel-node tools/build.dll.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watch",
    "analyze": "babel-node ./tools/analyzeBundle.js"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.6",
    "date-fns": "^1.28.5",
    "debug": "^3.1.0",
    "js-cookie": "^2.1.2",
    "lodash": "^4.16.6",
    "antd": "3.12.4",
    "prop-types": "^15.5.10",
    "query-string": "^4.2.2",
    "react": "^16.1.1",
    "react-amap": "^1.0.3",
    "react-dom": "^16.1.1",
    "react-dom-factories": "^1.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.0-rc.1",
    "react-sortable-hoc": "^1.3.1",
    "redux": "^3.5.2",
    "redux-thunk": "^1.0.3",
    "ua-parser-js": "^0.7.19"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^10.5.4",
    "@types/react": "16.4.7",
    "@types/react-dom": "^16.0.6",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "browser-sync": "^2.18.13",
    "chalk": "^1.1.3",
    "colors": "^1.1.2",
    "columnify": "^1.5.4",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "deep-equal": "^1.0.1",
    "ejs-loader": "^0.3.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "glob": "^7.0.5",
    "happypack": "^5.0.0",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "image-size": "^0.6.2",
    "imagemin": "5.3.1",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-pngquant": "5.0.1",
    "isparta": "^4.0.0",
    "jest": "^21.0.2",
    "jest-cli": "^21.0.2",
    "json-server": "^0.12.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.0",
    "lint-staged": "^7.3.0",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-run-all": "3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^1.3.0",
    "postcss-loader": "^2.1.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "2.5.4",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.0.0",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.1",
    "uuid": "^3.1.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "tools/**/*.js": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "src/lib/**/*.js",
      "src/lib/**/*.less"
    ],
    "globOptions": {
      "matchBase": true,
      "dot": true
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "testURL": "http://localhost:4000"
  },
  "author": "morrain",
  "license": "MIT"
}
