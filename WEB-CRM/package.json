    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.1.12",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/react": "16.4.17",
    "@types/react-dom": "16.0.9",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "browser-sync": "^2.18.13",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "colors": "^1.1.2",
    "columnify": "^1.5.4",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.5.4",
    "coveralls": "^2.13.1",
    "css-loader": "^1.0.0",
    "docdash": "^0.4.0",
    "ejs-loader": "^0.3.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^5.0.1",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "freemarker.js": "git+https://github.com/NSFI/freemarker.js.git",
    "glob": "^7.0.5",
    "happypack": "^5.0.0",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^1.3.1",
    "image-size": "^0.6.2",
    "imagemin": "5.3.1",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-pngquant": "5.0.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "isparta": "^4.0.0",
    "jest": "^21.0.2",
    "jest-cli": "^21.0.2",
    "jsdoc": "^3.5.5",
    "json-server": "^0.12.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.0",
    "lint-staged": "^8.1.5",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-run-all": "3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^1.3.0",
    "postcss-loader": "^2.1.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.0.0",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.5.8",
    "uuid": "^3.1.0",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "tools/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "source/**/*.less": [
        "stylelint --fix",
        "git add"
      ]
    },
    "globOptions": {
      "matchBase": true,
      "dot": true
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "testURL": "http://localhost:4000",
    "setupFiles": [
      "<rootDir>/source/__mock__/rAF.js",
      "<rootDir>/source/__mock__/localStorage.js"
    ]
  },
  "author": "",
  "license": "MIT"
}
