{
  "name": "react-ppfish-boilerplate",
  "version": "1.0.0",
  "description": "react ppfish boilerplate",
  "main": "index.js",
  "scripts": {
    "imagemin": "babel-node tools/imagemin.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node tools/mock.js",
    "start:debug": "babel-node $NODE_DEBUG_OPTION tools/srcServer.js",
    "open:src": "babel-node --max_old_space_size=4048 tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "build": "babel-node --max_old_space_size=4048 tools/build.js",
    "build:dll": "babel-node --max_old_space_size=4048 tools/build.dll.js",
    "stylelint": "stylelint \"source/**/*.less\"",
    "lint": "esw webpack.config.* source tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watch",
    "generate-docs": "jsdoc -c ./doc-conf.json -R ./README.md",
    "neibuild":"nei build -k fab54b3759463d14e32021f352fd8541",
    "neiserver":"nei server -k fab54b3759463d14e32021f352fd8541"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.6",
    "debug": "^3.1.0",
    "echarts": "^4.1.0",
    "js-cookie": "^2.1.2",
    "lodash": "^4.16.6",
    "lodash.debounce": "^4.0.8",
    "ppfish": "1.7.0-alpha.2",
    "prop-types": "^15.5.10",
    "query-string": "^4.2.2",
    "react": "^16.1.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.1.1",
    "react-dom-factories": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
